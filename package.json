{
  "name": "admin-pro",
  "version": "0.0.0",
  "description": "",
  "author": "lalifeier",
  "email": "lalifeier@gmail.com",
  "homepage": "https://lalifeier.github.io",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "build": "cross-env NODE_ENV=production vite build && rimraf dist/**/*.js.map",
    "build:test": "cross-env vite build --mode test",
    "build:no-cache": "yarn clean:cache && yarn build",
    "report": "cross-env REPORT=true yarn build",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "yarn build && vite preview",
    "preview:dist": "vite preview",
    "lint": "yarn lint:eslint && yarn lint:stylelint && yarn lint:prettier",
    "lint:eslint": "eslint --cache --max-warnings 0 --fix src/**/*.{js,jsx,ts,tsx,vue}",
    "lint:prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,css,less,scss,vue,html,md,json}",
    "lint:stylelint": "stylelint --cache --fix src/**/*.{html,vue,css,sass,scss,less}",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "ls-lint": "npx @ls-lint/ls-lint",
    "commit": "git-cz",
    "release": "standard-version",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "is-ci || husky install",
    "postinstall": "npx husky install",
    "gen:icon": "esno ./build/generate/icon.ts",
    "msw": "npx msw init public/ --save",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "clean:sourcemap": "rimraf dist/**/*.js.map",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && yarn install"
  },
  "dependencies": {
    "@iconify/iconify": "^2.0.4",
    "@mdi/font": "6.4.95",
    "@sentry/browser": "^6.14.1",
    "@sentry/tracing": "^6.14.1",
    "@types/crypto-js": "^4.0.2",
    "@vueuse/core": "^6.8.0",
    "animate.css": "^4.1.1",
    "axios": "^0.24.0",
    "clipboard": "^2.0.8",
    "cropperjs": "^1.5.12",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.7",
    "echarts": "^5.2.2",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "2.0.2",
    "print-js": "^1.6.0",
    "qs": "^6.10.1",
    "resize-observer-polyfill": "^1.5.1",
    "roboto-fontface": "*",
    "spark-md5": "^3.0.2",
    "spinkit": "^2.0.1",
    "tinymce": "^5.10.1",
    "vite-plugin-sentry": "^1.0.10",
    "vue": "^3.2.2",
    "vue-router": "^4.0.11",
    "vuetify": "^3.0.0-alpha.11",
    "xlsx": "^0.17.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@iconify/json": "^1.1.424",
    "@iconify/types": "^1.0.10",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-legacy": "^1.6.2",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/compiler-sfc": "^3.2.21",
    "autoprefixer": "^10.4.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.1.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "esno": "^0.11.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^11.2.6",
    "msw": "^0.35.0",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "sass": "^1.43.4",
    "standard-version": "^9.3.2",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.0.0",
    "typescript": "^4.4.4",
    "vite": "2.6.13",
    "vite-plugin-banner": "^0.1.3",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-imagemin": "^0.4.6",
    "vite-plugin-purge-icons": "^0.7.0",
    "vite-plugin-pwa": "^0.11.3",
    "vite-plugin-style-import": "^1.3.0",
    "vite-plugin-svg-icons": "^1.0.5",
    "vite-plugin-theme": "^0.8.1",
    "vite-plugin-windicss": "^1.5.1",
    "vue-cli-plugin-vuetify": "~2.4.3",
    "vue-tsc": "^0.28.10",
    "windicss": "^3.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "msw": {
    "workerDirectory": "public"
  },
  "engines": {
    "node": "^12 || >=14"
  }
}
