{
  "name": "vite-demo",
  "version": "0.0.0",
  "description": "",
  "author": "lalifeier",
  "email": "lalifeier@gmail.com",
  "homepage": "https://lalifeier.github.io",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "report": "cross-env REPORT=true yarn build ",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lint:js": "eslint --fix src/**/*.{js,jsx,ts,tsx,vue}",
    "lint:prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,css,less,scss,vue,html,md,json}",
    "lint:style": "stylelint --fix src/**/*.{html,vue,css,sass,scss,less}",
    "commit": "git-cz",
    "release": "standard-version",
    "prepare": "is-ci || husky install",
    "postinstall": "npx husky install",
    "ls-lint": "npx @ls-lint/ls-lint",
    "gen:icon": "esno ./build/generate/icon.ts",
    "msw": "npx msw init public/ --save"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{html,vue,css,sass,scss,less}": [
      "stylelint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx,css,less,scss,vue,html,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@iconify/iconify": "^2.0.1",
    "@types/crypto-js": "^4.0.1",
    "@vueuse/core": "^5.0.2",
    "animate.css": "^4.1.1",
    "axios": "^0.21.1",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.10.5",
    "jquery": "^3.6.0",
    "lodash-es": "^4.17.21",
    "mitt": "^2.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.0-alpha.13",
    "qs": "^6.10.1",
    "vue": "^3.0.5",
    "vue-router": "^4.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@iconify/json": "^1.1.355",
    "@iconify/types": "^1.0.6",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@vitejs/plugin-legacy": "^1.4.1",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vitejs/plugin-vue-jsx": "^1.1.5",
    "@vue/compiler-sfc": "^3.1.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.11.1",
    "esno": "^0.7.2",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.0.0",
    "msw": "^0.29.0",
    "prettier": "^2.3.1",
    "rollup-plugin-visualizer": "^5.5.0",
    "sass": "^1.34.1",
    "standard-version": "^9.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.7",
    "vite-plugin-banner": "^0.1.3",
    "vite-plugin-compression": "^0.2.5",
    "vite-plugin-imagemin": "^0.3.2",
    "vite-plugin-purge-icons": "^0.7.0",
    "vite-plugin-pwa": "^0.8.1",
    "vite-plugin-style-import": "^0.10.1",
    "vite-plugin-svg-icons": "^0.7.0",
    "vite-plugin-theme": "^0.8.1",
    "vite-plugin-windicss": "^1.0.3",
    "vue-tsc": "^0.1.7",
    "windicss": "^3.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
